{
    "name": "sysbox/laravel-base-entity",
    "description": "A base model for non-numerical id and common attributes: id, active, created, created_at, updated, updated_at",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "helin",
            "email": "lin.he@sysbox.com.au"
        }
    ],
    "minimum-stability": "dev",
    "keywords": ["Laravel", "BaseEntity"],
    "require": {
        "illuminate/support": "~5|~6|~7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Sysbox\\LaravelBaseEntity\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sysbox\\LaravelBaseEntity\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sysbox\\LaravelBaseEntity\\LaravelBaseEntityServiceProvider"
            ],
            "aliases": {
                "LaravelBaseEntity": "Sysbox\\LaravelBaseEntity\\Facades\\LaravelBaseEntity"
            }
        }
    }
}
